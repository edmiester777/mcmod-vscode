{
    "name": "mcmodcrafter",
    "displayName": "MCMod Crafter",
    "description": "A tool to develop mods for Minecraft.",
    "version": "0.0.1",
    "publisher": "edmiester777",
    "engines": {
        "vscode": "^1.28.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "mcmod.showHome",
                "title": "Show MCMod Home"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.4.2",
        "mdbootstrap": "^4.5.13",
        "rete": "^1.0.0-alpha.12",
        "rete-area-plugin": "^0.2.0",
        "rete-connection-plugin": "^0.2.1",
        "rete-vue-render-plugin": "^0.2.4",
        "smartwizard": "^4.3.1",
        "vue": "^2.5.17"
    }
}
